---
import PageLayout from '../layouts/PageLayout.astro';

import { getEntry } from 'astro:content';
import DataCard from '../components/DataCard.astro';

const { data } = await getEntry('data', 'data');
---

<PageLayout title="Data">
  <div class="space-y-3 px-0 md:px-6">
    {data.map((d) => <DataCard title={d.title} link={d.link} description={d.description} />)}
  </div>
</PageLayout>
